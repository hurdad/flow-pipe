{
  "swagger": "2.0",
  "info": {
    "title": "flowpipe/v1/service.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "FlowService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/flows": {
      "get": {
        "summary": "List all flows.",
        "operationId": "FlowService_ListFlows",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ListFlowsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "FlowService"
        ]
      },
      "post": {
        "summary": "Create a new flow (version starts at 1).",
        "operationId": "FlowService_CreateFlow",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Flow"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "spec",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1FlowSpec"
            }
          }
        ],
        "tags": [
          "FlowService"
        ]
      }
    },
    "/v1/flows/{name}": {
      "get": {
        "summary": "Get the active version of a flow.",
        "operationId": "FlowService_GetFlow",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Flow"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "FlowService"
        ]
      },
      "delete": {
        "summary": "Delete a flow and tear down its runtime.",
        "operationId": "FlowService_DeleteFlow",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "FlowService"
        ]
      },
      "put": {
        "summary": "Update an existing flow (creates a new version).",
        "operationId": "FlowService_UpdateFlow",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Flow"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "spec",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1FlowSpec"
            }
          }
        ],
        "tags": [
          "FlowService"
        ]
      }
    },
    "/v1/flows/{name}/rollback/{version}": {
      "post": {
        "summary": "Roll back to a previous immutable version.",
        "operationId": "FlowService_RollbackFlow",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Flow"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "version",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "uint64"
          }
        ],
        "tags": [
          "FlowService"
        ]
      }
    },
    "/v1/flows/{name}/status": {
      "get": {
        "summary": "Get status only (fast path, no spec mutation).",
        "operationId": "FlowService_GetFlowStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1FlowStatus"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "FlowService"
        ]
      }
    }
  },
  "definitions": {
    "LoggingConfigLogProcessorType": {
      "type": "string",
      "enum": [
        "LOG_PROCESSOR_UNSPECIFIED",
        "LOG_PROCESSOR_SIMPLE",
        "LOG_PROCESSOR_BATCH"
      ],
      "default": "LOG_PROCESSOR_UNSPECIFIED",
      "description": " - LOG_PROCESSOR_UNSPECIFIED: Runtime default.\n - LOG_PROCESSOR_SIMPLE: Synchronous export.\n - LOG_PROCESSOR_BATCH: Asynchronous batch export."
    },
    "ObservabilityConfigLoggingConfig": {
      "type": "object",
      "properties": {
        "otlpEndpoint": {
          "type": "string",
          "description": "Optional OTLP endpoint override for logs (host:port).\nIf unset, the global runtime logging endpoint is used."
        },
        "transport": {
          "$ref": "#/definitions/v1OtlpTransport",
          "description": "OTLP transport selection (gRPC or HTTP).\nIf unspecified, runtime default is used."
        },
        "processor": {
          "$ref": "#/definitions/LoggingConfigLogProcessorType",
          "description": "Log processor selection."
        },
        "batch": {
          "$ref": "#/definitions/ObservabilityConfigLoggingConfigBatchConfig",
          "description": "Batch processor configuration."
        }
      },
      "title": "==========================================================\nLogging configuration\n=========================================================="
    },
    "ObservabilityConfigLoggingConfigBatchConfig": {
      "type": "object",
      "properties": {
        "maxQueueSize": {
          "type": "integer",
          "format": "int64",
          "title": "Max queued log records"
        },
        "maxExportBatchSize": {
          "type": "integer",
          "format": "int64",
          "title": "Max records per export"
        },
        "scheduleDelayMs": {
          "type": "integer",
          "format": "int64",
          "title": "Delay before export"
        },
        "exportTimeoutMs": {
          "type": "integer",
          "format": "int64",
          "title": "Export timeout"
        }
      }
    },
    "ObservabilityConfigMetricsConfig": {
      "type": "object",
      "properties": {
        "otlpEndpoint": {
          "type": "string",
          "description": "Optional OTLP endpoint override for metrics (host:port).\nIf unset, the global runtime metrics endpoint is used."
        },
        "transport": {
          "$ref": "#/definitions/v1OtlpTransport",
          "description": "OTLP transport selection (gRPC or HTTP).\nIf unspecified, runtime default is used."
        },
        "stageMetricsEnabled": {
          "type": "boolean",
          "description": "Emit per-stage metrics."
        },
        "queueMetricsEnabled": {
          "type": "boolean",
          "description": "Emit queue-level metrics."
        },
        "flowMetricsEnabled": {
          "type": "boolean",
          "description": "Emit flow-level aggregate metrics."
        },
        "latencyHistogramsEnabled": {
          "type": "boolean",
          "description": "Emit latency histogram metrics."
        },
        "countersOnly": {
          "type": "boolean",
          "description": "Emit counters only (no gauges or histograms)."
        },
        "minCollectionIntervalMs": {
          "type": "integer",
          "format": "int64",
          "description": "Minimum metrics emission interval (milliseconds)."
        },
        "collectionIntervalMs": {
          "type": "integer",
          "format": "int64",
          "description": "Preferred metrics emission interval (milliseconds)."
        },
        "latencyBudgetMs": {
          "type": "integer",
          "format": "int64",
          "description": "Expected latency budget for metric tagging and alerting."
        }
      },
      "title": "==========================================================\nMetrics configuration\n=========================================================="
    },
    "ObservabilityConfigTracingConfig": {
      "type": "object",
      "properties": {
        "otlpEndpoint": {
          "type": "string",
          "description": "Optional OTLP endpoint override for tracing (host:port).\nIf unset, the global runtime tracing endpoint is used."
        },
        "transport": {
          "$ref": "#/definitions/v1OtlpTransport",
          "description": "OTLP transport selection (gRPC or HTTP).\nIf unspecified, runtime default is used."
        },
        "traceHint": {
          "$ref": "#/definitions/TracingConfigTraceHint",
          "description": "Sampling intent hint for this flow."
        },
        "processor": {
          "$ref": "#/definitions/TracingConfigTraceProcessorType",
          "description": "Trace processor selection."
        },
        "batch": {
          "$ref": "#/definitions/ObservabilityConfigTracingConfigBatchConfig",
          "description": "Batch processor configuration."
        },
        "stageSpansEnabled": {
          "type": "boolean",
          "description": "Emit spans for stage execution."
        },
        "queueSpansEnabled": {
          "type": "boolean",
          "description": "Emit spans for queue wait and dequeue."
        },
        "recordSpansEnabled": {
          "type": "boolean",
          "description": "Emit per-record spans (high overhead)."
        },
        "attributeLevel": {
          "$ref": "#/definitions/TracingConfigAttributeLevel",
          "description": "Attribute verbosity level for tracing."
        },
        "latencyBudgetMs": {
          "type": "integer",
          "format": "int64",
          "description": "Expected end-to-end latency budget (milliseconds).\nUsed for observability context only (not enforced)."
        }
      },
      "title": "==========================================================\nTracing configuration\n=========================================================="
    },
    "ObservabilityConfigTracingConfigBatchConfig": {
      "type": "object",
      "properties": {
        "maxQueueSize": {
          "type": "integer",
          "format": "int64",
          "description": "Maximum number of spans queued in memory.\nSpans are dropped once this limit is reached."
        },
        "maxExportBatchSize": {
          "type": "integer",
          "format": "int64",
          "description": "Maximum number of spans sent per export.\nMust be \u003c= max_queue_size."
        },
        "scheduleDelayMs": {
          "type": "integer",
          "format": "int64",
          "description": "Delay between two consecutive exports (milliseconds)."
        },
        "exportTimeoutMs": {
          "type": "integer",
          "format": "int64",
          "description": "Maximum time allowed for an export attempt (milliseconds).\nNOTE: Currently ignored by the OTEL C++ SDK but preserved\nfor forward compatibility."
        }
      }
    },
    "TracingConfigAttributeLevel": {
      "type": "string",
      "enum": [
        "ATTRIBUTES_UNSPECIFIED",
        "ATTRIBUTES_MINIMAL",
        "ATTRIBUTES_STANDARD",
        "ATTRIBUTES_VERBOSE"
      ],
      "default": "ATTRIBUTES_UNSPECIFIED",
      "description": " - ATTRIBUTES_UNSPECIFIED: Runtime default.\n - ATTRIBUTES_MINIMAL: IDs and timing only.\n - ATTRIBUTES_STANDARD: Safe default attributes.\n - ATTRIBUTES_VERBOSE: Debug-level verbosity."
    },
    "TracingConfigTraceHint": {
      "type": "string",
      "enum": [
        "TRACE_HINT_UNSPECIFIED",
        "TRACE_HINT_DEFAULT",
        "TRACE_HINT_ALWAYS",
        "TRACE_HINT_NEVER"
      ],
      "default": "TRACE_HINT_UNSPECIFIED",
      "description": " - TRACE_HINT_UNSPECIFIED: Use runtime default behavior.\n - TRACE_HINT_DEFAULT: Normal sampling behavior.\n - TRACE_HINT_ALWAYS: Force tracing for this flow.\n - TRACE_HINT_NEVER: Suppress tracing for this flow."
    },
    "TracingConfigTraceProcessorType": {
      "type": "string",
      "enum": [
        "TRACE_PROCESSOR_UNSPECIFIED",
        "TRACE_PROCESSOR_SIMPLE",
        "TRACE_PROCESSOR_BATCH"
      ],
      "default": "TRACE_PROCESSOR_UNSPECIFIED",
      "description": " - TRACE_PROCESSOR_UNSPECIFIED: Runtime default.\n - TRACE_PROCESSOR_SIMPLE: Synchronous export.\n - TRACE_PROCESSOR_BATCH: Asynchronous batch export."
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "protobufNullValue": {
      "type": "string",
      "enum": [
        "NULL_VALUE"
      ],
      "default": "NULL_VALUE",
      "description": "`NullValue` is a singleton enumeration to represent the null value for the\n`Value` type union.\n\nThe JSON representation for `NullValue` is JSON `null`.\n\n - NULL_VALUE: Null value."
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1CpuSet": {
      "type": "object",
      "properties": {
        "cpu": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          },
          "description": "CPU core identifiers."
        }
      }
    },
    "v1Execution": {
      "type": "object",
      "properties": {
        "mode": {
          "$ref": "#/definitions/v1ExecutionMode",
          "description": "Selected execution mode."
        }
      }
    },
    "v1ExecutionMode": {
      "type": "string",
      "enum": [
        "EXECUTION_MODE_UNSPECIFIED",
        "EXECUTION_MODE_STREAMING",
        "EXECUTION_MODE_JOB"
      ],
      "default": "EXECUTION_MODE_UNSPECIFIED",
      "description": " - EXECUTION_MODE_UNSPECIFIED: Execution mode not specified.\n - EXECUTION_MODE_STREAMING: Long-running streaming pipeline.\n - EXECUTION_MODE_JOB: Run-to-completion batch job."
    },
    "v1Flow": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Unique flow identifier."
        },
        "version": {
          "type": "string",
          "format": "uint64",
          "description": "Current spec version."
        },
        "spec": {
          "$ref": "#/definitions/v1FlowSpec",
          "description": "Desired configuration."
        },
        "status": {
          "$ref": "#/definitions/v1FlowStatus",
          "description": "Observed runtime state."
        }
      }
    },
    "v1FlowSpec": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Logical flow name."
        },
        "version": {
          "type": "string",
          "format": "uint64",
          "description": "Immutable version assigned by the controller."
        },
        "image": {
          "type": "string",
          "description": "Container image when using container runtime."
        },
        "execution": {
          "$ref": "#/definitions/v1Execution",
          "description": "Execution semantics for the flow."
        },
        "stages": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1StageSpec"
          },
          "description": "Ordered pipeline stages."
        },
        "queues": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1QueueSpec"
          },
          "description": "Queues connecting stages."
        },
        "cpuPinning": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/v1CpuSet"
          },
          "description": "Optional CPU affinity per stage."
        },
        "resources": {
          "$ref": "#/definitions/v1Resources",
          "description": "Aggregate resource intent."
        },
        "labels": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "description": "User-defined metadata labels."
        },
        "observability": {
          "$ref": "#/definitions/v1ObservabilityConfig",
          "description": "Flow-level observability overrides."
        },
        "kubernetes": {
          "$ref": "#/definitions/v1KubernetesSettings",
          "description": "Kubernetes runtime settings."
        }
      }
    },
    "v1FlowState": {
      "type": "string",
      "enum": [
        "FLOW_STATE_UNSPECIFIED",
        "FLOW_STATE_PENDING",
        "FLOW_STATE_DEPLOYING",
        "FLOW_STATE_RUNNING",
        "FLOW_STATE_SUCCEEDED",
        "FLOW_STATE_FAILED",
        "FLOW_STATE_STOPPED"
      ],
      "default": "FLOW_STATE_UNSPECIFIED",
      "description": " - FLOW_STATE_UNSPECIFIED: State not specified.\n - FLOW_STATE_PENDING: Awaiting scheduling or resources.\n - FLOW_STATE_DEPLOYING: Being deployed or updated.\n - FLOW_STATE_RUNNING: Actively running.\n - FLOW_STATE_SUCCEEDED: Completed successfully (jobs only).\n - FLOW_STATE_FAILED: Execution failed.\n - FLOW_STATE_STOPPED: Explicitly stopped."
    },
    "v1FlowStatus": {
      "type": "object",
      "properties": {
        "state": {
          "$ref": "#/definitions/v1FlowState",
          "description": "Current lifecycle state."
        },
        "message": {
          "type": "string",
          "description": "Human-readable status message."
        },
        "activeVersion": {
          "type": "string",
          "format": "uint64",
          "description": "Currently active version."
        },
        "workload": {
          "type": "string",
          "description": "Kubernetes workload name."
        },
        "lastUpdated": {
          "type": "string",
          "format": "date-time",
          "description": "Last controller reconciliation time."
        }
      }
    },
    "v1ImagePullPolicy": {
      "type": "string",
      "enum": [
        "IMAGE_PULL_POLICY_UNSPECIFIED",
        "IMAGE_PULL_POLICY_ALWAYS",
        "IMAGE_PULL_POLICY_IF_NOT_PRESENT",
        "IMAGE_PULL_POLICY_NEVER"
      ],
      "default": "IMAGE_PULL_POLICY_UNSPECIFIED",
      "description": "Kubernetes image pull policy.\n\n - IMAGE_PULL_POLICY_UNSPECIFIED: Pull policy not specified (controller default).\n - IMAGE_PULL_POLICY_ALWAYS: Always pull the image.\n - IMAGE_PULL_POLICY_IF_NOT_PRESENT: Pull if not present on node.\n - IMAGE_PULL_POLICY_NEVER: Never pull the image."
    },
    "v1KubernetesSettings": {
      "type": "object",
      "properties": {
        "imagePullPolicy": {
          "$ref": "#/definitions/v1ImagePullPolicy",
          "description": "Image pull policy for runtime pods."
        },
        "restartPolicy": {
          "$ref": "#/definitions/v1RestartPolicy",
          "description": "Restart policy for runtime pods."
        }
      },
      "description": "Kubernetes runtime settings for flow workloads."
    },
    "v1ListFlowsResponse": {
      "type": "object",
      "properties": {
        "flows": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1Flow"
          }
        }
      }
    },
    "v1ObservabilityConfig": {
      "type": "object",
      "properties": {
        "metricsEnabled": {
          "type": "boolean",
          "description": "Allow metrics emission for this flow."
        },
        "tracingEnabled": {
          "type": "boolean",
          "description": "Allow tracing emission for this flow."
        },
        "logsEnabled": {
          "type": "boolean",
          "description": "Allow log emission for this flow."
        },
        "tracing": {
          "$ref": "#/definitions/ObservabilityConfigTracingConfig",
          "description": "Tracing configuration block."
        },
        "metrics": {
          "$ref": "#/definitions/ObservabilityConfigMetricsConfig",
          "description": "Metrics configuration block."
        },
        "logging": {
          "$ref": "#/definitions/ObservabilityConfigLoggingConfig",
          "description": "Logging configuration block."
        },
        "debug": {
          "type": "boolean",
          "description": "Indicates this flow is under investigation and may emit\nhigher-cost observability signals."
        }
      }
    },
    "v1OtlpTransport": {
      "type": "string",
      "enum": [
        "OTLP_TRANSPORT_UNSPECIFIED",
        "OTLP_TRANSPORT_GRPC",
        "OTLP_TRANSPORT_HTTP"
      ],
      "default": "OTLP_TRANSPORT_UNSPECIFIED",
      "description": "Defines which OTLP transport should be used when exporting\ntelemetry. This is a hint only and may be ignored by runtime\npolicy.\n\n - OTLP_TRANSPORT_UNSPECIFIED: Runtime default (typically gRPC)\n - OTLP_TRANSPORT_GRPC: OTLP over gRPC\n - OTLP_TRANSPORT_HTTP: OTLP over HTTP/protobuf",
      "title": "------------------------------------------------------------\nOTLP transport selection\n------------------------------------------------------------"
    },
    "v1QueueSpec": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Queue name."
        },
        "capacity": {
          "type": "integer",
          "format": "int64",
          "description": "Maximum buffered elements."
        }
      }
    },
    "v1Resources": {
      "type": "object",
      "properties": {
        "cpuCores": {
          "type": "integer",
          "format": "int64",
          "description": "Preferred total CPU cores."
        },
        "memoryMb": {
          "type": "integer",
          "format": "int64",
          "description": "Preferred memory allocation in MB."
        },
        "profile": {
          "type": "string",
          "description": "Named resource profile hint."
        }
      }
    },
    "v1RestartPolicy": {
      "type": "string",
      "enum": [
        "RESTART_POLICY_UNSPECIFIED",
        "RESTART_POLICY_ALWAYS",
        "RESTART_POLICY_ON_FAILURE",
        "RESTART_POLICY_NEVER"
      ],
      "default": "RESTART_POLICY_UNSPECIFIED",
      "description": "Kubernetes restart policy for runtime pods.\n\n - RESTART_POLICY_UNSPECIFIED: Restart policy not specified (controller default).\n - RESTART_POLICY_ALWAYS: Always restart the pod.\n - RESTART_POLICY_ON_FAILURE: Restart on failure.\n - RESTART_POLICY_NEVER: Never restart the pod."
    },
    "v1StageSpec": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Unique stage name within the flow."
        },
        "type": {
          "type": "string",
          "description": "Stage type identifier."
        },
        "threads": {
          "type": "integer",
          "format": "int64",
          "description": "Number of worker threads."
        },
        "inputQueue": {
          "type": "string",
          "description": "Input queue name (if any)."
        },
        "outputQueue": {
          "type": "string",
          "description": "Output queue name (if any)."
        },
        "config": {
          "type": "object",
          "title": "Opaque, plugin-owned config"
        },
        "plugin": {
          "type": "string",
          "description": "Optional plugin implementation override."
        }
      }
    }
  }
}
