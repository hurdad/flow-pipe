      "type": "object",
      "properties": {
        "metricsEnabled": {
          "type": "boolean"
        },
        "tracingEnabled": {
          "type": "boolean"
        },
        "logsEnabled": {
          "type": "boolean"
        },
        "otlpEndpoint": {
          "type": "string"
        },
        "transport": {
          "$ref": "#/definitions/v1OtlpTransport"
        },
        "tracing": {
          "$ref": "#/definitions/ObservabilityConfigTracingConfig"
        },
        "metrics": {
          "$ref": "#/definitions/ObservabilityConfigMetricsConfig"
        },
        "logging": {
          "$ref": "#/definitions/ObservabilityConfigLoggingConfig"
        },
        "debug": {
          "type": "boolean"
        }
      }
    },
    "v1OtlpTransport": {
      "type": "string",
      "enum": [
        "OTLP_TRANSPORT_UNSPECIFIED",
        "OTLP_TRANSPORT_GRPC",
        "OTLP_TRANSPORT_HTTP"
      ],
      "default": "OTLP_TRANSPORT_UNSPECIFIED",
      "description": "- OTLP_TRANSPORT_UNSPECIFIED: Runtime default (typically gRPC)\n - OTLP_TRANSPORT_GRPC: OTLP over gRPC\n - OTLP_TRANSPORT_HTTP: OTLP over HTTP/protobuf",
      "title": "------------------------------------------------------------\nOTLP transport selection\n------------------------------------------------------------"
    },
    "v1QueueSchema": {
      "type": "object",
      "properties": {
        "format": {
          "$ref": "#/definitions/v1QueueSchemaFormat",
          "description": "Schema format identifier."
        },
        "schemaId": {
          "type": "string",
          "description": "Schema identifier."
        },
        "version": {
          "type": "integer",
          "format": "int64",
          "description": "Schema version in the registry."
        },
        "registryUrl": {
          "type": "string",
          "description": "Optional registry base URL override."
        }
      }
    },
