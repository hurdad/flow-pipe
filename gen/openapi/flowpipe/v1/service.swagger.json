          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Flow"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "spec",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1FlowSpec"
            }
          }
        ],
        "tags": [
          "FlowService"
        ]
      }
    },
    "/v1/flows/{name}/rollback/{version}": {
