        },
        "maxExportBatchSize": {
          "type": "integer",
          "format": "int64"
        },
        "scheduleDelayMs": {
          "type": "integer",
          "format": "int64"
        },
        "exportTimeoutMs": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "ObservabilityConfigMetricsConfig": {
      "type": "object",
      "properties": {
        "stageMetricsEnabled": {
          "type": "boolean",
          "title": "Granularity controls"
        },
        "queueMetricsEnabled": {
          "type": "boolean"
        },
        "flowMetricsEnabled": {
          "type": "boolean"
        },
        "latencyHistogramsEnabled": {
          "type": "boolean",
          "title": "Cost / cardinality controls"
        },
        "countersOnly": {
          "type": "boolean"
        },
        "minCollectionIntervalMs": {
          "type": "integer",
          "format": "int64",
          "title": "Collection interval hints"
        },
        "collectionIntervalMs": {
          "type": "integer",
          "format": "int64"
        },
        "latencyBudgetMs": {
          "type": "integer",
          "format": "int64"
        }
      },
      "title": "==========================================================\nMetrics configuration\n=========================================================="
    },
    "ObservabilityConfigTracingConfig": {
      "type": "object",
      "properties": {
        "traceHint": {
          "$ref": "#/definitions/TracingConfigTraceHint"
        },
        "processor": {
          "$ref": "#/definitions/TracingConfigTraceProcessorType"
        },
        "batch": {
          "$ref": "#/definitions/ObservabilityConfigTracingConfigBatchConfig"
        },
        "stageSpansEnabled": {
          "type": "boolean",
          "title": "--------------------------------------------------------\nSpan granularity controls\n--------------------------------------------------------"
        },
        "queueSpansEnabled": {
          "type": "boolean"
        },
        "recordSpansEnabled": {
          "type": "boolean"
        },
        "attributeLevel": {
          "$ref": "#/definitions/TracingConfigAttributeLevel"
        },
        "latencyBudgetMs": {
          "type": "integer",
          "format": "int64"
        }
      },
      "title": "==========================================================\nTracing configuration\n=========================================================="
    },
    "ObservabilityConfigTracingConfigBatchConfig": {
      "type": "object",
      "properties": {
        "maxQueueSize": {
          "type": "integer",
          "format": "int64"
        },
        "maxExportBatchSize": {
          "type": "integer",
          "format": "int64"
        },
        "scheduleDelayMs": {
          "type": "integer",
          "format": "int64"
        },
        "exportTimeoutMs": {
          "type": "integer",
          "format": "int64"
        }
      },
      "title": "--------------------------------------------------------\nBatch trace processor tuning\n--------------------------------------------------------"
    },
    "TracingConfigAttributeLevel": {
      "type": "string",
      "enum": [
        "ATTRIBUTES_UNSPECIFIED",
        "ATTRIBUTES_MINIMAL",
        "ATTRIBUTES_STANDARD",
        "ATTRIBUTES_VERBOSE"
      ],
      "default": "ATTRIBUTES_UNSPECIFIED",
      "title": "--------------------------------------------------------\nTrace attribute verbosity\n--------------------------------------------------------"
    },
    "TracingConfigTraceHint": {
      "type": "string",
      "enum": [
        "TRACE_HINT_UNSPECIFIED",
        "TRACE_HINT_DEFAULT",
        "TRACE_HINT_ALWAYS",
        "TRACE_HINT_NEVER"
      ],
      "default": "TRACE_HINT_UNSPECIFIED",
      "title": "--------------------------------------------------------\nTracing sampling intent\n--------------------------------------------------------"
    },
    "TracingConfigTraceProcessorType": {
      "type": "string",
      "enum": [
        "TRACE_PROCESSOR_UNSPECIFIED",
        "TRACE_PROCESSOR_SIMPLE",
        "TRACE_PROCESSOR_BATCH"
      ],
      "default": "TRACE_PROCESSOR_UNSPECIFIED",
      "title": "--------------------------------------------------------\nTrace processing strategy\n--------------------------------------------------------"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "protobufNullValue": {
      "type": "string",
      "enum": [
        "NULL_VALUE"
      ],
      "default": "NULL_VALUE",
      "description": "`NullValue` is a singleton enumeration to represent the null value for the\n`Value` type union.\n\n The JSON representation for `NullValue` is JSON `null`.\n\n - NULL_VALUE: Null value."
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1CpuSet": {
      "type": "object",
      "properties": {
        "cpu": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          },
          "description": "CPU core identifiers."
        }
      }
    },
    "v1Execution": {
      "type": "object",
      "properties": {
        "mode": {
          "$ref": "#/definitions/v1ExecutionMode",
          "description": "Selected execution mode."
        }
      }
    },
    "v1ExecutionMode": {
      "type": "string",
      "enum": [
        "EXECUTION_MODE_UNSPECIFIED",
        "EXECUTION_MODE_STREAMING",
        "EXECUTION_MODE_JOB"
      ],
      "default": "EXECUTION_MODE_UNSPECIFIED",
      "description": " - EXECUTION_MODE_UNSPECIFIED: Execution mode not specified.\n - EXECUTION_MODE_STREAMING: Long-running streaming pipeline.\n - EXECUTION_MODE_JOB: Run-to-completion batch job."
    },
    "v1Flow": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Unique flow identifier."
        },
        "version": {
          "type": "string",
          "format": "uint64",
          "description": "Current spec version."
        },
        "spec": {
          "$ref": "#/definitions/v1FlowSpec",
          "description": "Desired configuration."
        },
        "status": {
          "$ref": "#/definitions/v1FlowStatus",
          "description": "Observed runtime state."
        }
      }
    },
    "v1FlowSpec": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Logical flow name."
        },
        "version": {
          "type": "string",
          "format": "uint64",
          "description": "Immutable version assigned by the controller."
        },
        "execution": {
          "$ref": "#/definitions/v1Execution",
          "description": "Execution semantics for the flow."
        },
        "stages": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1StageSpec"
          },
          "description": "Ordered pipeline stages."
        },
        "queues": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1QueueSpec"
          },
          "description": "Queues connecting stages."
        },
        "labels": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "description": "User-defined metadata labels."
        },
        "observability": {
          "$ref": "#/definitions/v1ObservabilityConfig",
          "description": "Flow-level observability overrides."
        },
        "kubernetes": {
          "$ref": "#/definitions/v1KubernetesSettings",
          "description": "Kubernetes runtime settings."
        }
      }
    },
    "v1FlowState": {
      "type": "string",
      "enum": [
        "FLOW_STATE_UNSPECIFIED",
        "FLOW_STATE_PENDING",
        "FLOW_STATE_DEPLOYING",
        "FLOW_STATE_RUNNING",
        "FLOW_STATE_SUCCEEDED",
        "FLOW_STATE_FAILED",
        "FLOW_STATE_STOPPED"
      ],
      "default": "FLOW_STATE_UNSPECIFIED",
      "description": " - FLOW_STATE_UNSPECIFIED: State not specified.\n - FLOW_STATE_PENDING: Awaiting scheduling or resources.\n - FLOW_STATE_DEPLOYING: Being deployed or updated.\n - FLOW_STATE_RUNNING: Actively running.\n - FLOW_STATE_SUCCEEDED: Completed successfully (jobs only).\n - FLOW_STATE_FAILED: Execution failed.\n - FLOW_STATE_STOPPED: Explicitly stopped."
    },
    "v1FlowStatus": {
      "type": "object",
      "properties": {
        "state": {
          "$ref": "#/definitions/v1FlowState",
          "description": "Current lifecycle state."
        },
        "message": {
          "type": "string",
          "description": "Human-readable status message."
        },
        "activeVersion": {
          "type": "string",
          "format": "uint64",
          "description": "Currently active version."
        },
        "workload": {
          "type": "string",
          "description": "Kubernetes workload name."
        },
        "lastUpdated": {
          "type": "string",
          "format": "date-time",
          "description": "Last controller reconciliation time."
        }
      }
    },
    "v1ImagePullPolicy": {
      "type": "string",
      "enum": [
        "IMAGE_PULL_POLICY_UNSPECIFIED",
        "IMAGE_PULL_POLICY_ALWAYS",
        "IMAGE_PULL_POLICY_IF_NOT_PRESENT",
        "IMAGE_PULL_POLICY_NEVER"
      ],
      "default": "IMAGE_PULL_POLICY_UNSPECIFIED",
      "description": "Kubernetes image pull policy.\n\n - IMAGE_PULL_POLICY_UNSPECIFIED: Pull policy not specified (controller default).\n - IMAGE_PULL_POLICY_ALWAYS: Always pull the image.\n - IMAGE_PULL_POLICY_IF_NOT_PRESENT: Pull if not present on node.\n - IMAGE_PULL_POLICY_NEVER: Never pull the image."
    },
    "v1KubernetesSettings": {
      "type": "object",
      "properties": {
        "image": {
          "type": "string",
          "description": "Container image when using container runtime."
        },
        "imagePullPolicy": {
          "$ref": "#/definitions/v1ImagePullPolicy",
          "description": "Image pull policy for runtime pods."
        },
        "restartPolicy": {
          "$ref": "#/definitions/v1RestartPolicy",
          "description": "Restart policy for runtime pods."
        },
        "cpuPinning": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/v1CpuSet"
          },
          "description": "Optional CPU affinity per stage."
        },
        "resources": {
          "$ref": "#/definitions/v1Resources",
          "description": "Aggregate resource intent."
        }
      },
      "description": "Kubernetes runtime settings for flow workloads."
    },
    "v1ListFlowsResponse": {
      "type": "object",
      "properties": {
        "flows": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1Flow"
          }
        }
      }
    },
    "v1ListSchemaVersionsResponse": {
