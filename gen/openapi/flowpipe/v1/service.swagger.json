      "type": "string",
      "enum": [
        "QUEUE_SCHEMA_FORMAT_UNSPECIFIED",
        "QUEUE_SCHEMA_FORMAT_AVRO",
        "QUEUE_SCHEMA_FORMAT_JSON",
        "QUEUE_SCHEMA_FORMAT_PROTOBUF",
        "QUEUE_SCHEMA_FORMAT_FLATBUFFERS",
        "QUEUE_SCHEMA_FORMAT_PARQUET"
      ],
      "default": "QUEUE_SCHEMA_FORMAT_UNSPECIFIED",
      "description": " - QUEUE_SCHEMA_FORMAT_UNSPECIFIED: Format not specified.\n - QUEUE_SCHEMA_FORMAT_AVRO: Apache Avro schema.\n - QUEUE_SCHEMA_FORMAT_JSON: JSON Schema.\n - QUEUE_SCHEMA_FORMAT_PROTOBUF: Protocol Buffers.\n - QUEUE_SCHEMA_FORMAT_FLATBUFFERS: FlatBuffers schema.\n - QUEUE_SCHEMA_FORMAT_PARQUET: Apache Parquet schema."
    },
    "v1QueueSpec": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
